citHeader("To cite StreamLight in publications use:")

citEntry(
  entry    = "Manual",
  author   = as.person("Phil Savoy"),
  title    = "StreamLight: An R package for estimating stream lighting conditions",
  note = "R package version 0.2.0",
  year     = "2021",
  url      = "https://github.com/psavoy/StreamLight",
  textVersion = paste("Savoy, P. (2021). StreamLight: An R package for estimating stream lighting conditions. R package version 0.2.0. https://github.com/psavoy/StreamLight"

  )
)

